table GDP
	lineageTag: 6bcac93b-49f7-4808-85e1-aa33f0539acf

	measure 'Total GDP' = SUM(GDP[GDP (Billions)])*1000000000
		formatString: \$#,0.0;(\$#,0.0);\$#,0.0
		lineageTag: 26aee8c2-5094-4a62-b2ea-c087f78d3597

		annotation PBI_FormatHint = {"currencyCulture":"en-US"}

	measure 'YoY GDP Growth %' = ```
			
			VAR CurrentYearGDP = [Total GDP]
			VAR PreviousYr = MAX(GDP[Year]) - 1
			VAR PreviousYearGDP = 
			    CALCULATE(
			        [Total GDP],
			        GDP[Year] = PreviousYr
			    )
			RETURN
			    IF(
			        NOT(ISBLANK(PreviousYearGDP)),
			        DIVIDE(CurrentYearGDP - PreviousYearGDP, PreviousYearGDP) * 100,
			        BLANK()
			    )
			```
		lineageTag: bad6aec1-3aac-4295-bb17-b721f5d25a3b

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'GDP Rank' =
			
			RANKX(
			    ALLSELECTED(GDP[Country]),
			    [Total GDP],
			    ,
			    DESC,
			    Dense
			)
		formatString: 0
		lineageTag: 1309fbef-e1ca-4599-b061-13354bfcb2bc

